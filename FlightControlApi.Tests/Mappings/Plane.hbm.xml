<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="FlightControlApi" namespace="FlightControlApi.Models">
  <class name="Plane" table="Plane" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="long">
      <generator class="native" />
    </id>

    <property name="Model"  />
    <property name="SerialNumber" />
    <property name="EconomyCapacity" />
    <property name="BusinessCapacity" />
    <property name="FirstClassCapacity" />
    <property name="Active" />
  </class>
</hibernate-mapping>