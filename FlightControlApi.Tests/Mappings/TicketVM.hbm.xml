<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="FlightControlApi" namespace="FlightControlApi.Models">
  <class name="TicketVM" table="Ticket" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" column="Id" type="long">
      <generator class="native" />
    </id>

    <property name="Price"  />
    <property name="SeatId" />
    <property name="FlightId" />
    <property name="StoreId" />
    <property name="PassengerId" />
    <property name="Revoked" />
    <many-to-one name="Flight" lazy="false"  class="FlightVM" column="FlightId" cascade="save-update"/>
    <many-to-one name="Store" lazy="false"  class="StoreVM" column="StoreId" cascade="save-update"/>
    <many-to-one name="Seat" lazy="false"  class="Seat" column="SeatId" cascade="save-update"/>
    <many-to-one name="Passenger" lazy="false"  class="PassengerVM" column="PassengerId" cascade="save-update"/>


  </class>
</hibernate-mapping>